<Window x:Class="NewsDashboard.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NewsDashboard" xmlns:news="clr-namespace:NewsDashboard.News"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TabControl>
            <TabItem>
                <TabItem.Header>News</TabItem.Header>
                <news:News_View></news:News_View>
            </TabItem>
            <TabItem>
                <TabItem.Header>
                    RSS/Comics
                </TabItem.Header>
            </TabItem>
            <TabItem>
                <TabItem.Header>About</TabItem.Header>
            </TabItem>
        </TabControl>
    </Grid>
</Window>
